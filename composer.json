{
	"name": "tangram/tangram",
	"version": "0.7.0",
	"type": "library",
	"description": "Make predictions with a Tangram model from your PHP app. Learn more at https://www.tangram.dev/.",
	"keywords": [
		"tangram"
	],
	"homepage": "https://www.tangram.dev",
	"license": "MIT",
	"authors": [
		{
			"name": "Tangram",
			"email": "root@tangram.dev",
			"homepage": "https://www.tangram.dev",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"phpunit/phpunit": ">=8.0",
		"squizlabs/php_codesniffer": "^3.0",
		"friendsofphp/php-cs-fixer": "^3.2"
	},
	"autoload": {
		"psr-4": {
			"tangram\\tangram\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"tangram\\tangram\\": "tests"
		}
	},
	"scripts": {
		"test": "phpunit",
		"check-style": "phpcs src tests",
		"fix-style": "phpcbf src tests"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"config": {
		"sort-packages": true
	}
}
